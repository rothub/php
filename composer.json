{
    "name": "rothub/php",
    "type": "library",
    "description": "PHP",
    "license": "MIT",
    "authors": [
        {
            "name": "sean",
            "email": "sxfmao@163.com"
        }
    ],
    "require": {
        "php": ">=8.0.0",
        "ext-openssl": "*",
        "ext-simplexml": "*",
        "guzzlehttp/guzzle": "^7.1",
        "monolog/monolog": "^2.1",
        "pimple/pimple": "^3.3",
        "psr/simple-cache": "^1.0",
        "symfony/cache": "^5.1",
        "symfony/http-foundation": "^5.1",
        "wechatpay/wechatpay": "^1.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.4"
    },
    "autoload": {
        "psr-4": {
            "RotHub\\PHP\\": "src"
        },
        "files": [
            "functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "RotHub\\PHP\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "repositories": {
        "packagist": {
            "type": "composer",
            "url": "https://mirrors.aliyun.com/composer"
        }
    }
}
